{
    "env": {
        "node": true
    },
    "parserOptions": {
        "ecmaVersion": 2017,
        "sourceType": "module",
        "ecmaFeatures": {
            "experimentalObjectRestSpread": true
        }
    },
    "rules": {
        "no-console": "warn",
        "no-duplicate-case": "off",
        "no-unused-vars": "warn"
    }
}
